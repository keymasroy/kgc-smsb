{
  "name": "@kgc/smsb-bo",
  "version": "1.0.0",
  "main": "index.js",
  "author": "GSITM",
  "license": "MIT",
  "imports": {
    "#ustra/core": "@ustra/core/src",
    "#ustra/core/*": "@ustra/core/src/*",
    "#ustra/h3": "@ustra/h3/src",
    "#ustra/h3/*": "@ustra/h3/src/*",
    "#ustra/nuxt": "@ustra/nuxt/src",
    "#ustra/nuxt/*": "@ustra/nuxt/src/*",
    "#ustra/nuxt-vuetify": "@ustra/nuxt-vuetify/src",
    "#ustra/nuxt-vuetify/*": "@ustra/nuxt-vuetify/src/*",
    "#ustra/nuxt-wijmo": "@ustra/nuxt-wijmo/src",
    "#ustra/nuxt-wijmo/*": "@ustra/nuxt-wijmo/src/*",
    "#kgc/smsb-common": "@kgc/smsb-common/src",
    "#kgc/smsb-common/*": "@kgc/smsb-common/src/*"
  },
  "scripts": {
    "clean": "nuxt cleanup",
    "dev": "cross-env CONFIG_ENV=local nuxt dev --force -p 11061",
    "generate": "cross-env CONFIG_ENV=dev nuxt clean && nuxt generate",
    "prepare": "nuxt prepare",
    "boot": "yarn install && node ./node_modules/@ustra/core/boot",
    "preinstall": "cd ../common && yarn install",
    "postinstall": "yarn link \"@kgc/smsb-common\""
  },
  "resolutions": {
    "highlight.js": "11.7.0"
  },
  "devDependencies": {
    "@ustra/core": "3.0.0-snapshot.12",
    "@ustra/nuxt": "3.0.0-snapshot.12",
    "@ustra/nuxt-vuetify": "3.0.0-snapshot.12",
    "@ustra/nuxt-ckeditor5": "3.0.0-snapshot.12",
    "@ustra/nuxt-wijmo": "3.0.0-snapshot.12"
  }
}

{
  "compilerOptions": {
    "jsx": "preserve",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "strict": false,
    "allowJs": true,
    "noEmit": false,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "declarationMap": true,
    "declaration": true,
    "downlevelIteration": true,
    "types": ["@types/node", "@types/jest", "nuxt"],
    "baseUrl": ".",
    "paths": {
      "#ustra/core": ["./node_modules/@ustra/core/src"],
      "#ustra/core/*": ["./node_modules/@ustra/core/src/*"],
      "#ustra/h3": ["./node_modules/@ustra/h3/src"],
      "#ustra/h3/*": ["./node_modules/@ustra/h3/src/*"],
      "#ustra/nuxt": ["./node_modules/@ustra/nuxt/src"],
      "#ustra/nuxt/*": ["./node_modules/@ustra/nuxt/src/*"],
      "#ustra/nuxt-wijmo": ["./node_modules/@ustra/nuxt-wijmo/src"],
      "#ustra/nuxt-wijmo/*": ["./node_modules/@ustra/nuxt-wijmo/src/*"],
      "#ustra/nuxt-vuetify": ["./node_modules/@ustra/nuxt-vuetify/src"],
      "#ustra/nuxt-vuetify/*": ["./node_modules/@ustra/nuxt-vuetify/src/*"],

      "#app": ["./node_modules/nuxt/dist/app"],
      "#app/*": ["./node_modules/nuxt/dist/app/*"],
      "vue-demi": ["./node_modules/nuxt/dist/app/compat/vue-demi"],
      "#head": ["./node_modules/nuxt/dist/head/runtime"],
      "#head/*": ["./node_modules/nuxt/dist/head/runtime/*"]
    }
  },
  "ts-node": {
    "require": ["tsconfig-paths/register"],
    "compilerOptions": {
      "module": "commonjs"
    }
  }
}
